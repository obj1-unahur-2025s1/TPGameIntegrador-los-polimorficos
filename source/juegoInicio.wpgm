import escenario.*
import wollok.game.*
import cachito.*
import musicaFondo.*
import escenas.*

program bindingOfCachitto {
	game.title("The Binding of Cachito")
	game.height(16)
	game.width(11)
	game.cellSize(50)
	cachito.configurarTeclas()
	inicio.iniciar()
	game.onCollideDo(cachito, {objeto => objeto.interaccion()}) 
	keyboard.r().onPressDo({inicio.reiniciar() })
	game.start()
}
